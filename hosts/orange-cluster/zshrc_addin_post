export JAVA_HOME="/usr/lib/jvm/java-8-openjdk-amd64"
export YARN_HOME="/home/ubuntu/hadoop"
export HADOOP_HOME="/home/ubuntu/hadoop"
export PATH="$YARN_HOME/sbin:$YARN_HOME/bin:$PATH"

alias run-mr-caida='./bin/run.sh \
  -job_id mr_caida \
  -user_main edu.snu.nemo.examples.beam.MapReduce \
  -dag_dir "./dag/mr_caida" \
  -user_args "hdfs://master:9000/caida hdfs://master:9000/output" \
  -deploy_mode yarn'
